<?xml version="1.0"?>
<cd:Record xmlns:cd="http://www.stcorp.nl/coda/definition/2008/07" xmlns:ct="http://www.stcorp.nl/coda/test/2008/10" format="binary" last-modified="2008-07-01" name="Level_1A_Mie_Measurement_MDSR_03_05">
  <cd:Field name="start_of_observation_time">
    <cd:Time timeformat="binary_envisat_datetime">
      <cd:Description>Start time at start of BRC</cd:Description>
      <ct:NamedTest id="RangeSecond" path="seconds"/>
      <ct:NamedTest id="RangeMicroSecond" path="microseconds"/>
      <ct:NamedTest id="TimeNotZero"/>
    </cd:Time>
  </cd:Field>
  <cd:Field name="num_measurements">
    <cd:Integer>
      <cd:Description>Number of measurements</cd:Description>
      <cd:BitSize>16</cd:BitSize>
      <cd:NativeType>uint16</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="num_height_bins">
    <cd:Integer>
      <cd:Description>Number of height bins</cd:Description>
      <cd:BitSize>16</cd:BitSize>
      <cd:NativeType>uint16</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="num_accd_columns">
    <cd:Integer>
      <cd:Description>Number of ACCD colums</cd:Description>
      <cd:BitSize>16</cd:BitSize>
      <cd:NativeType>uint16</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="mie_measurement_data">
    <cd:Array>
      <cd:Dimension>1675</cd:Dimension>
      <cd:Dimension>20</cd:Dimension>
      <cd:Integer>
        <cd:Description>Spectrometer counts</cd:Description>
        <cd:BitSize>16</cd:BitSize>
        <cd:NativeType>uint16</cd:NativeType>
      </cd:Integer>
    </cd:Array>
  </cd:Field>
</cd:Record>
