<?xml version="1.0"?>
<cd:Record xmlns:cd="http://www.stcorp.nl/coda/definition/2008/07" xmlns:ct="http://www.stcorp.nl/coda/test/2008/10" format="binary" last-modified="2018-04-03" name="Level_2A_MCA_PCD_ADSR_03_02">
  <cd:Field name="starttime">
    <cd:Time timeformat="binary_envisat_datetime">
      <cd:Description>Start date and time of the MCA profile covered by the present DSR. This is the centroid time of the first measurement of the profile.</cd:Description>
      <ct:NamedTest id="RangeSecond" path="seconds"/>
      <ct:NamedTest id="RangeMicroSecond" path="microseconds"/>
    </cd:Time>
  </cd:Field>
  <cd:Field name="processing_qc_flag_bin">
    <cd:Array>
      <cd:Dimension>24</cd:Dimension>
      <cd:Integer>
        <cd:Description>Last Mie bin that has been processed</cd:Description>
        <cd:BitSize>8</cd:BitSize>
        <cd:NativeType>uint8</cd:NativeType>
      </cd:Integer>
    </cd:Array>
  </cd:Field>
</cd:Record>
